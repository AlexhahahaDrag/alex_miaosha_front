{
  "experimentalTernaries": true, 
  "printWidth": 80, 
  "tabWidth": 2,
  "useTabs": true, 
  "semi": true, 
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false, 
  "trailingComma": "all", 
  "bracketSpacing": true, 
  "bracketSameLine": true, 
